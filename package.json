{
  "name": "project-boxbox",
  "version": "1.0.0",
  "main": "index.js",
  "author": "frantic_adulting <frantic.adulting@gmail.com>",
  "license": "MIT",
  "scripts": {
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "start-server": "docker-compose -f docker-compose.dev.yml up --build --force-recreate",
    "start": "nodemon src/api/index.ts",
    "compile": "tsc"
  },
  "scriptsComments": {
    "db:push": "Deploys database schema changes to the database. Meant for rapid iteration.",
    "db:migrate": "Commits the db schema changes. Creates a migration file.",
    "db:reset": "Resets the database and applies all migrations.",
    "compile": "Compiles Typescript (noEmit should be set to false inside tsconfig to avoid generating js files)."
  },
  "_moduleAliases": {
    "@root": "dist",
    "@api": "dist/api",
    "@config:": "dist/config",
    "@database": "dist/database",
    "@error": "dist/error",
    "@logger": "dist/logger"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/verror": "^1.10.6",
    "bullmq": "^3.5.9",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.9.0",
    "ts-results": "^3.3.0",
    "uuid": "^9.0.0",
    "verror": "^1.10.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "inversify": "^6.0.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prettier-plugin-prisma": "^4.10.0",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
